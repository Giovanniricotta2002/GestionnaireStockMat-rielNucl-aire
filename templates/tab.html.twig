<table id="test">
	<thead>
		<tr>
			{% for name in dataKeys   %}
				<th scope="col">{{name}}</th>
			{% endfor %}
			{% if action %}
				<th scope="col">Action</th>
			{% endif %}
		</tr>
	</thead>
	<tbody>
		{% for data in datas %}
            {% block row %}
                <tr>
                    <td scope="row"></td>
                </tr>
            {% endblock %}
		{% endfor %}
	</tbody>
</table>
